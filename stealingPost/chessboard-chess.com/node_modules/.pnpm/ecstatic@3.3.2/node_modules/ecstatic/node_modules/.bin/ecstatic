#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/eduardoworrel/Documents/PROGRAMADOR.TV/EVENTOS/steal/game-client.chess-tournaments/node_modules/.pnpm/ecstatic@3.3.2/node_modules/ecstatic/lib/node_modules:/Users/eduardoworrel/Documents/PROGRAMADOR.TV/EVENTOS/steal/game-client.chess-tournaments/node_modules/.pnpm/ecstatic@3.3.2/node_modules/ecstatic/node_modules:/Users/eduardoworrel/Documents/PROGRAMADOR.TV/EVENTOS/steal/game-client.chess-tournaments/node_modules/.pnpm/ecstatic@3.3.2/node_modules:/Users/eduardoworrel/Documents/PROGRAMADOR.TV/EVENTOS/steal/game-client.chess-tournaments/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/eduardoworrel/Documents/PROGRAMADOR.TV/EVENTOS/steal/game-client.chess-tournaments/node_modules/.pnpm/ecstatic@3.3.2/node_modules/ecstatic/lib/node_modules:/Users/eduardoworrel/Documents/PROGRAMADOR.TV/EVENTOS/steal/game-client.chess-tournaments/node_modules/.pnpm/ecstatic@3.3.2/node_modules/ecstatic/node_modules:/Users/eduardoworrel/Documents/PROGRAMADOR.TV/EVENTOS/steal/game-client.chess-tournaments/node_modules/.pnpm/ecstatic@3.3.2/node_modules:/Users/eduardoworrel/Documents/PROGRAMADOR.TV/EVENTOS/steal/game-client.chess-tournaments/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/ecstatic.js" "$@"
else
  exec node  "$basedir/../../lib/ecstatic.js" "$@"
fi
